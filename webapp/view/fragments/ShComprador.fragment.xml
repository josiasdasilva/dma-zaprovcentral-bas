<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Comprador"
		class="sapUiPopupWithPadding"
		items="{/CompradorSet}"
		liveChange="onValueHelpCompradorSearch"
		confirm="onValueHelpCompradorClose"
		cancel="onValueHelpCompradorClose"
        multiSelect="false">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Ekgrp}"
			description="{Nome}"
			info="{Uname}" />
	</SelectDialog>
</core:FragmentDefinition>
<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Fornecedor"
		class="sapUiPopupWithPadding"
		items="{/FornecedorSet}"
		liveChange="onValueHelpFornecedorSearch"
		confirm="onValueHelpFornecedorClose"
		cancel="onValueHelpFornecedorClose"
        multiSelect="false">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Lifnr}"
			description="{Mcod1}"
			info="{Ekgrp}" />
	</SelectDialog>
</core:FragmentDefinition>